{
	"update_url": "http://clients2.google.com/service/update2/crx",
	"name": "tinyFilter",
	"version": "0.3",
	"description": "tinyFilter: The Web content filter for Google Chrome!",
	"icons": {
		"48": "48.png",
		"128": "128.png"
	},
	"browser_action": {
		"default_icon": "48.png",
		"popup": "popup.html"
	},
	"permissions": [
		"http://*/*", "https://*/*", "tabs"
	],
	"background_page": "background.html",	
	"options_page": "options.html",
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": [
				"site.js",
				"content.js"
			],
			"run_at": "document_start",
			"all_frames": true
		},{
			"matches": ["http://*/*", "https://*/*"],
			"js": [
				"overlay.js" 
			],
			"run_at": "document_end",
			"all_frames": true
		}
	]
}

